{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "operator-linebreak": 0,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "no-restricted-globals": 0,
    "indent": 0
  }
}
